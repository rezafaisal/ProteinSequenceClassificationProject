\name{GenerateSegments}
\alias{GenerateSegments}
\title{GenerateSegments}
\description{
This function generate adjacent and overlapped segments from a protein sequence
}
\usage{
GenerateSegments(seq_str, k, is.original = FALSE)
}
\arguments{
\item{seq_str}{Protein sequence.}
\item{k}{Divider value. If k=2, it will generate 2 adjacent segments and 1 overlapped segment.}
\item{is.original}{Option to add original sequence in result.}
}
\examples{
GenerateSegments("RSVTTEINTLFQTLTSIAEKVDP", 3)
}
